        <script>
            $(function() {
                const msgList = JSON.parse('{{{json msgList}}}' || '[]' )

                socket.on('user.toast', function(data) {
                    console.log('test')
                    handleMessage(data.type, data.msg)
                })

                msgList.forEach((msg) => {
                    handleMessage(msg.type, msg.msg)
                })


                function handleMessage(type = String, msg = String) {
                    type = type.toLowerCase();

                    switch(type) {
                        case "success":
                            showSuccessToast(msg);
                            break;
                        case "warning": 
                            showWarningToast(msg);
                            break;
                        case "danger":
                            showDangerToast(msg);
                            break;
                        default:
                            showInfoToast(msg);
                            break;
                    }
                }

                function showSuccessToast(text) {
                    'use strict';
                    resetToastPosition();
                    $.toast({
                        heading: 'Success',
                        text: text,
                        showHideTransition: 'slide',
                        icon: 'success',
                        loaderBg: '#f96868',
                        position: 'top-right'
                    })
                };
                function showInfoToast(text) {
                    'use strict';
                    resetToastPosition();
                    $.toast({
                        heading: 'Info',
                        text: text,
                        showHideTransition: 'slide',
                        icon: 'info',
                        loaderBg: '#46c35f',
                        position: 'top-right'
                    })
                };
                function showWarningToast(text) {
                    'use strict';
                    resetToastPosition();
                    $.toast({
                        heading: 'Warning',
                        text: text,
                        showHideTransition: 'slide',
                        icon: 'warning',
                        loaderBg: '#57c7d4',
                        position: 'top-right'
                    })
                };
                function showDangerToast(text) {
                    'use strict';
                    resetToastPosition();
                    $.toast({
                        heading: 'Danger',
                        text: text,
                        showHideTransition: 'slide',
                        icon: 'error',
                        loaderBg: '#f2a654',
                        position: 'top-right'
                    })
                };
                function resetToastPosition() {
                    $('.jq-toast-wrap').removeClass('bottom-left bottom-right top-left top-right mid-center'); // to remove previous position class
                    $(".jq-toast-wrap").css({
                        "top": "",
                        "left": "",
                        "bottom": "",
                        "right": ""
                    }); //to remove previous position style
                };
            })
        </script>
        
